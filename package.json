{
  "name": "adr-gateway",
  "version": "1.0.8",
  "description": "A gateway ready to interact with the Australian Consumer Data Right ecosystem. Allows Accredited Data Recipients to quickly deploy their software products and participate in the CDR without needing to develop the complexities of boiler-plate data recipient interactions.",
  "author": "Regional Australia Bank",
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "base64url": "^3.0.1",
    "body-parser": "^1.19.0",
    "class-validator": "^0.11.0",
    "convict": "^5.2.0",
    "cors": "^2.8.5",
    "entropy-string": "^4.2.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-validator": "^6.2.0",
    "html-entities": "^1.2.1",
    "tunnel": "^0.0.6",
    "jose": "^1.10.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "tsyringe": "^3.4.0",
    "typeorm": "^0.2.19",
    "typescript": "3.7.x",
    "url-join": "^4.0.1",
    "url-parse": "^1.4.7",
    "uuid": "^3.3.3",
    "winston": "^3.2.1",
    "qs": "^6.9.1",
    "node-forge": "^0.9.1",
    "seedrandom": "^3.0.5",
    "flat": "^5.0.0",
    "oidc-provider": "^6.17.4",
    "http-proxy": "^1.18.0"
  },
  "scripts": {
    "build": "npx rimraf dist && npx tsc && npx cpx package.json dist",
    "clean": "npx rimraf dist",
    "evidence-clean": "npx rimraf .evidence",
    "start:pm2": "pm2 delete examples/deployment/pm2/ecosystem.config.js && pm2 start examples/deployment/pm2/ecosystem.config.js",
    "start": "npm run build && npm run start:pm2",
    "start:docker": "docker-compose -f ./examples/deployment/docker/docker-compose.yml up --build",
    "build:docker": "npm run copy-proj && docker build --build-arg HTTP_PROXY --build-arg HTTPS_PROXY examples/deployment/docker -t adr-gateway",
    "test": "cross-env TEST_SUITE_HEADLESS=1 mocha dist/Tests/**/*spec.js",
    "unit-test": "mocha dist/Tests/Unit/**/*.spec.js --reporter mochawesome",
    "copy-proj": "npx rimraf examples/deployment/docker/.work && npx cpx src/** examples/deployment/docker/.work && npx cpx {package,tsconfig}.json examples/deployment/docker/.work/"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/chai-arrays": "^1.0.3",
    "@types/chai-as-promised": "^7.1.2",
    "@types/chai-spies": "^1.0.1",
    "@types/convict": "^4.2.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/flat": "0.0.28",
    "@types/lodash": "^4.14.144",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.12",
    "@types/node-forge": "^0.9.3",
    "@types/qs": "^6.9.1",
    "@types/url-join": "^4.0.0",
    "@types/url-parse": "^1.4.3",
    "@types/uuid": "^3.4.5",
    "@types/validatorjs": "^3.15.0",
    "chai": "^4.2.0",
    "chai-arrays": "^2.0.0",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.0.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.2",
    "get-port": "^5.1.1",
    "mocha": "^6.2.1",
    "mochawesome": "^4.1.0",
    "open": "^7.0.0",
    "puppeteer": "^>=2.1 <3",
    "puppeteer-har": "^1.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "sqlite3": "https://github.com/mapbox/node-sqlite3/tarball/master",
    "ts-node": "^8.4.1",
    "validatorjs": "^3.18.1"
  }
}

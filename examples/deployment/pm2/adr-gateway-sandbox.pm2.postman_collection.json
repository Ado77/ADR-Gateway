		{
			"name": "New Consent at specified Data Holder",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "fd4e2bb3-9774-48f0-96de-a0d224aa447d",
						"exec": [
							"pm.test(\"Consent request code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.collectionVariables.set(\"gatewayConsentId\", pm.response.json().consentId);",
							"",
							"pm.environment.set(\"gatewayConsentId\", pm.response.json().consentId);"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "basic",
					"basic": [
						{
							"key": "password",
							"value": "gateway-password",
							"type": "string"
						},
						{
							"key": "username",
							"value": "gateway-user",
							"type": "string"
						}
					]
				},
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n\t\"userId\": \"user1\",\n\t\"dataholderBrandId\":\"test-data-holder-1\",\n\t\"softwareProductId\":\"sandbox\",\n\t\"systemId\": \"sandbox\",\n\t\"scopes\": [\"bank:accounts.basic:read\",\"bank:accounts.detail:read\",\"bank:transactions:read\"],\n\t\"sharingDuration\": 86400,\n\t\"state\": \"string\"\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "https://localhost:9101/cdr/consents",
					"protocol": "https",
					"host": [
						"localhost"
					],
					"port": "9101",
					"path": [
						"cdr",
						"consents"
					]
				}
			},
			"response": []
		},
